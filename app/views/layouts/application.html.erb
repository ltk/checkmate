<!DOCTYPE html>
<html>
<head>
  <title>Checkmate</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |key,value| %>
  <% if value.present? %>
    <div class="flash">
      <%= value %>
    </div>
  <% end %>
<% end %>
<%= yield %>

</body>
</html>
